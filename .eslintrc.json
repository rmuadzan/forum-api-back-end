{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": 0,
        "no-underscore-dangle": "off",
        "class-methods-use-this": "warn",
        "no-unused-vars": "warn",
        "no-param-reassign": "warn",
        "camelcase": "off"
    }
}
